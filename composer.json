{
    "name": "lodash-php/lodash-php",
    "description": "A port of Lodash to PHP",
    "keywords": [
        "lodash",
        "utility",
        "utilities",
        "php-library",
        "utility-library",
        "php-utility"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Pierre du Plessis",
            "email": "open-source@solidworx.co"
        }
    ],
    "autoload": {
        "files": [
            "src/internal/unicode.php",
            "src/internal/Traits/CacheDataTrait.php",
            "src/CacheInterface.php",
            "src/bootstrap.php"
        ]
    },
    "require": {
        "php": "^7.1",
        "sebastian/comparator": "^1.2 | ^2.0 | ^2.1",
        "symfony/property-access": "^2.7 | ^3.0 | ^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "phpdocumentor/reflection-docblock": "^4.2"
    }
}
